javascript:(function(){
  let amount = parseInt(prompt("How many tabs do you want to open?"));
  if(isNaN(amount) || amount < 1) return alert("Invalid number");
  let url = prompt("What URL should the tabs open?");
  if(!url) return;

  for(let i = 0; i < amount; i++){
    window.open(url, '_blank');
  }
})();
